{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'frostbite-runtime-desktop'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=frostbite-runtime-desktop",
                    "--package=frostbite-runtime-desktop"
                ],
                "filter": {
                    "name": "frostbite-runtime-desktop",
                    "kind": "bin"
                }
            },
            "args": ["run", "examples/math.fsb"],
            "cwd": "${workspaceFolder}"
            
        }
    ]
}